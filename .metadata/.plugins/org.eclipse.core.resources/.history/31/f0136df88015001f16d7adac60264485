package jsonPack2ManojBro;

import java.util.List;

public class Department {
	private String departmentName;
	private List<EmployeeClass> employeeList;

	public Department() {
	}

	public Department(String departmentName, List<EmployeeClass> employeeList) {
		super();
		this.departmentName = departmentName;
		this.employeeList = employeeList;
	}

	public String getDepartmentName() {
		return departmentName;
	}

	public void setDepartmentName(String departmentName) {
		this.departmentName = departmentName;
	}

	public List<EmployeeClass> getEmployeeList() {
		return employeeList;
	}

	public void setEmployeeList(List<EmployeeClass> employeeList) {
		this.employeeList = employeeList;
	}

	@Override
	public String toString() {
		return "DepartmentClass [departmentName=" + departmentName + ", employeeList=" + employeeList + "]";
	}

}
