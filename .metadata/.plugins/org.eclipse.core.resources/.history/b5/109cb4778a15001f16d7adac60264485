package jsonPack2ManojBro;

import java.util.List;

public class Company {
	private String companyName;
	private List<DepartmentHarii> departments;

	public Company() {
	}

	public Company(String companyName, List<DepartmentHarii> departments) {
		super();
		this.companyName = companyName;
		this.departments = departments;
	}

	

	public String getCompanyName() {
		return companyName;
	}

	public void setCompanyName(String name) {
		this.companyName = name;
	}

	public List<DepartmentHarii> getDepartments() {
		return departments;
	}

	public void setDepartments(List<DepartmentHarii> departments) {
		this.departments = departments;
	}

	@Override
	public String toString() {
		return "Company [companyName=" + companyName + ", departments=" + departments + "]";
	}

}
