package jsonPack2ManojBro;

import java.util.List;

public class Company {
	private String companyName;
	private List<DepartmentClass> departments;

	public Company(String name, List<DepartmentClass> departments) {
		super();
		this.companyName = companyName;
		this.departments = departments;
	}

	public Company() {

	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public List<DepartmentClass> getDepartment() {
		return department;
	}

	public void setDepartment(List<DepartmentClass> department) {
		this.department = department;
	}

	@Override
	public String toString() {
		return "Company [name=" + name + ", department=" + department + "]";
	}

}
