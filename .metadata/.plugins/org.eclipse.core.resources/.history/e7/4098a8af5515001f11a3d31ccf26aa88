package jsonPack2ManojBro;

import java.util.List;

public class Company {
	private String companyName;
	private List<DepartmentClass> departments;

	public Company(String companyName, List<DepartmentClass> departments) {
		super();
		this.companyName = companyName;
		this.departments = departments;
	}

	public Company() {

	}

	public String getName() {
		return companyName;
	}

	public void setName(String companyName) {
		this.companyName = companyName;
	}

	public List<DepartmentClass> getDepartment() {
		return departments;
	}

	public void setDepartment(List<DepartmentClass> department) {
		this.departments = departments;
	}

	@Override
	public String toString() {
		return "Company [name=" + companyName + ", department=" + departments + "]";
	}

}
